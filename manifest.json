{
  "manifest_version": 3,
  "name": "SumMap",
  "version": "0.1",
  "description": "Summarizes web pages and generates mind maps.",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "*://*.wikipedia.org/*",
    "https://*.googleapis.com/*"
  ]
}
